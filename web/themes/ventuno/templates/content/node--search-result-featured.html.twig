{#
/**
 * @file
 * Theme override to display a node.
 */
#}
{% block content %}
  {{ pattern('card', {
    'variants': 'horizontal',
    'title': {
      attributes: [
        {
          name: "href",
          value: url,
        },
      ],
      'content': label
    },
    'content': content,
    'image': image,
  }) }}
{% endblock %}
