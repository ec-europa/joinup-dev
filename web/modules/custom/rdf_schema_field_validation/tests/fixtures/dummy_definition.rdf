         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <rdfs:Class rdf:about="http://example.com/dummy">
        <rdfs:comment xml:lang="en">An example class definition.</rdfs:comment>
        <rdfs:label xml:lang="en">Dummy</rdfs:label>
    </rdfs:Class>

    <rdf:Property rdf:about="http://purl.org/dc/terms/description">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
        <dc:identifier xml:lang="en">dct:description</dc:identifier>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
