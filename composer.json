{
    "name": "ec-europa/joinup",
    "description": "Joinup is a collaborative platform created by the European Commission and funded by the European Union via the Interoperability Solutions for European Public Administrations (ISA) Programme. It offers several services that aim to help e-Government professionals share their experience with each other.",
    "type": "project",
    "license": "EUPL",
    "require": {
        "composer/composer": "~1.0",
        "composer/installers": "^1.2",
        "continuousphp/aws-sdk-phing": "~0.1",
        "cweagans/composer-patches": "~1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/console": "~1.0",
        "drupal/core": "~8.3.0@alpha",
        "drupal/digital_size_formatter": "~1.0-alpha1",
        "drupal/ds": "^2.0",
        "drupal/facets": "~1.0@alpha",
        "drupal/field_group": "~1.0-rc6",
        "drupal/inline_entity_form": "dev-1.x#dbfc93fc961b3e6c84497390c62ec36d02c1c4d7",
        "drupal/message": "~1.0",
        "drupal/message_notify": "~1.0",
        "drupal/migrate_drush": "~1.1",
        "drupal/migrate_run": "~1.0-alpha1",
        "drupal/migrate_spreadsheet": "1.x-dev",
        "drupal/og": "dev-1.x#34a9425ffeeb267ceeba21261f4719d0ced36e58",
        "drupal/og_menu": "~1.0@alpha",
        "drupal/phingdrushtask": "dev-7.x-2.x#13428ee7a229da3b7e6be4687587048150b1fd25",
        "drupal/r4032login": "dev-1.x#391f10d69b9b8c466a2fddac2a6b99f1289e7781",
        "drupal/search_api": "~1.0@beta",
        "drupal/search_api_solr": "~1.0@beta",
        "drupal/state_machine": "~1.0-beta3",
        "drupal/token": "~1.0",
        "drupal/view_unpublished": "~1.0@alpha",
        "drush/drush": "~8.0",
        "easyrdf/easyrdf": "0.10.0-alpha1 as 0.9.2",
        "ec-europa/infra": "~0.5",
        "google/material-design-lite": "dev-v1.1.3",
        "minimaxir/big-list-of-naughty-strings": "dev-master",
        "pear/console_table": "~1.3",
        "phing/phing": "^2.15.0",
        "php": ">=5.5.9",
        "phpoffice/phpspreadsheet": "~0.0",
        "solarium/solarium": "~3.6",
        "typhonius/behattask": "1.0"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "behat/mink-selenium2-driver": "dev-master#721cbbaf37f71434f6acb604dc3a17a6a9be0c4c",
        "drupal/admin_toolbar": "~1.17",
        "drupal/coder": "~8.2.9",
        "drupal/config_devel": "dev-1.x#92f735cae8e9fb328edca285340a5bfbf72028cc",
        "drupal/config_update": "~1.2",
        "drupal/default_content": "dev-1.x#18f2408a0f74d871b076ef94d1b83f380ea797c9",
        "drupal/devel": "~1.0@alpha",
        "drupal/drupal-extension": "dev-master#8d2eea92396b7079f3f9ac6f67c8987aa1e911a5",
        "jcalderonzumba/gastonjs": "^1.1@dev",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "pear/http_request2": "~2.3",
        "pfrenssen/phpcs-pre-push": "1.1",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Drupal\\joinup\\": "tests/src",
            "DrupalProject\\": "src",
            "PhpOffice\\PhpSpreadsheet\\": "vendor/phpoffice/phpspreadsheet/src/PhpSpreadsheet"
        },
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"],
            "web/themes/joinup/vendor/{$name}": ["type:drupal-theme-library"]
        },
        "patches": {
            "drupal/core": {
                "Check permissions when 'Add terms' @see https://www.drupal.org/node/2845021": "https://www.drupal.org/files/issues/2845021-2.patch",
                "Don't compute children and parents of a new term @see https://www.drupal.org/node/2845361": "https://www.drupal.org/files/issues/2845361-2.patch",
                "Enlarge file_usage entity ID length. @see https://www.drupal.org/node/2675600": "https://www.drupal.org/files/issues/2675600-17.patch",
                "#2346893: Duplicate AJAX wrapper around a file field. @see https://www.drupal.org/node/2346893": "https://www.drupal.org/files/issues/duplicate_ajax_wrapper-2346893-194.patch",
                "SqlBase::prepareQuery() should be called also on count. @see https://www.drupal.org/node/2833060": "https://www.drupal.org/files/issues/sqlbase_preparequery-2833060-2.patch",
                "Support fast rollbacks. @see https://www.drupal.org/node/2821988": "https://www.drupal.org/files/issues/support_fast_rollbacks-2821988-30.patch"
            },
            "drupal/inline_entity_form": {
                "Required IEF forms for existing entities don't work. @see https://www.drupal.org/node/2830328": "https://www.drupal.org/files/issues/inline_entity_form-existing_reference_no_submit-2830328-5-D8.patch"
            },
            "drupal/search_api": {
                "Add entity type list cache tags. @see https://www.drupal.org/node/2753667": "https://www.drupal.org/files/issues/2753667-46.patch",
                "Views cached results are not taking into account the access check. @see https://www.drupal.org/node/2824640 @see https://www.drupal.org/node/2753667": "./resources/patches/views_cached_results-2824640-8-on_2753667-33.patch",
                "item_id column is too short. @see https://www.drupal.org/node/2829696": "https://www.drupal.org/files/issues/2829696-7.patch"
            },
            "drupal/search_api_solr": {
                "Cannot subscribe to Solarium events. @see https://www.drupal.org/node/2835770": "https://www.drupal.org/files/issues/cannot_subscribe_to-2835770-2.patch"
            },
            "drupal/state_machine": {
                "Fire generic events when transition are applied. @see https://www.drupal.org/node/2832415": "https://www.drupal.org/files/issues/fire_generic_events-2832415-2.patch",
                "Schema validation error when running functional tests. @see https://www.drupal.org/node/2838952": "https://www.drupal.org/files/issues/schema_validation_error-2838952-2.patch",
                "Disable static cache for workflow_callback. @see https://www.drupal.org/node/2846986": "https://www.drupal.org/files/issues/static_cache_disable_for_workflow_callback-2846986-2.patch"
            }
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "_comment": "The PhingDrushTask module does not currently have a D8 release and is not available on https://packages.drupal.org/8. See https://www.drupal.org/node/2817131.",
            "type": "vcs",
            "url": "https://git.drupal.org/project/phingdrushtask.git"
        },
        {
            "type": "package",
            "package": {
                "name": "typhonius/behattask",
                "version": "1.0",
                "source": {
                    "url": "https://gist.github.com/5719e15be9687ff7c138.git",
                    "type": "git",
                    "reference": "0ea666882cba3099dfa4feeb02e1bb85b2c0bde9"
                },
                "autoload": { "classmap": [ "BehatTask.php" ] },
                "include-path": [ "." ]
            }
        },
        {
            "type": "package",
            "package": {
                "name": "pfrenssen/phpcs-pre-push",
                "version": "1.1",
                "type": "git-hook",
                "source": {
                    "url": "https://gist.github.com/1ec87c25930861654cc94251da69f14b.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
        "type": "package",
            "package": {
                "name": "phpoffice/phpspreadsheet",
                "version": "0.0",
                "dist": {
                    "url": "https://github.com/PHPOffice/PhpSpreadsheet/archive/develop.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "google/material-design-lite",
                "version": "dev-v1.1.3",
                "type": "drupal-theme-library",
                "source": {
                    "url": "git@github.com:google/material-design-lite.git",
                    "type": "git",
                    "reference": "v1.1.3"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "minimaxir/big-list-of-naughty-strings",
                "version": "dev-master",
                "source": {
                    "url": "git@github.com:minimaxir/big-list-of-naughty-strings.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ]
}
