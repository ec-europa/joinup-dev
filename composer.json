                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "google/material-design-lite",
                "version": "dev-v1.1.3",
                "type": "drupal-theme-library",
                "source": {
                    "url": "git@github.com:google/material-design-lite.git",
