                "version": "dev-master",
                "source": {
                    "url": "git@github.com:SEMICeu/adms-ap_validator.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ec-europa/virtuoso",
