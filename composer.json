            }
        },
        {
            "type": "package",
            "package": {
                "name": "pfrenssen/phpcs-pre-push",
                "version": "1.1",
                "type": "git-hook",
                "source": {
                    "url": "https://gist.github.com/1ec87c25930861654cc94251da69f14b.git",
