{
    "name": "ec-europa/joinup",
    "description": "Joinup is a collaborative platform created by the European Commission and funded by the European Union via the Interoperability Solutions for European Public Administrations (ISA) Programme. It offers several services that aim to help e-Government professionals share their experience with each other.",
    "type": "project",
    "license": "EUPL",
    "require": {
        "composer/composer": "~1.0",
        "composer/installers": "^1.0.20",
        "continuousphp/aws-sdk-phing": "~0.1",
        "cweagans/composer-patches": "dev-master#5456199acdcc16b243814aa45259f4c56272a634",
        "drupal/console": "~1.0",
        "drupal/core": "^8.2.3",
        "drupal/digital_size_formatter": "~1.0-alpha1",
        "drupal/ds": "^2.0",
        "drupal/facets": "~1.0@alpha",
        "drupal/field_group": "dev-1.x#0f289425f4f1b156737fe27ae29dde2c55bbec27",
        "drupal/inline_entity_form": "dev-1.x",
        "drupal/message": "~1.0",
        "drupal/message_notify": "~1.0",
        "drupal/migrate_drush": "~1.1",
        "drupal/migrate_spreadsheet": "1.x-dev",
        "drupal/og": "dev-1.x#dbffd5d9123b2b5b0f0bc91efb3bc9d45da3f073",
        "drupal/og_menu": "dev-8.x-1.x",
        "drupal/phingdrushtask": "dev-7.x-2.x#13428ee7a229da3b7e6be4687587048150b1fd25",
        "drupal/r4032login": "dev-1.x#4b2077aa70e3f7b00b8a9cba25af5b948ba2e3b9",
        "drupal/search_api": "~1.0@beta",
        "drupal/search_api_solr": "~1.0@beta",
        "drupal/state_machine": "~1.0-beta3",
        "drupal/token": "~1.0",
        "drupal/view_unpublished": "dev-1.x#869c5b6b80f6badc0b39dff07ba852c9c967b9c7",
        "drush/drush": "~8.0",
        "easyrdf/easyrdf": "0.10.0-alpha1 as 0.9.2",
        "ec-europa/infra": "~0.5",
        "google/material-design-lite": "dev-v1.1.3",
        "pear/console_table": "~1.3",
        "phing/phing": "^2.15.0",
        "php": ">=5.5.9",
        "phpoffice/phpspreadsheet": "~0.0",
        "solarium/solarium": "~3.6",
        "typhonius/behattask": "1.0"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "behat/mink-selenium2-driver": "dev-master#721cbbaf37f71434f6acb604dc3a17a6a9be0c4c",
        "drupal-composer/drupal-scaffold": "^2.0.1",
        "drupal/admin_toolbar": "~1.17",
        "drupal/coder": "~8.2.9",
        "drupal/config_devel": "dev-1.x#92f735cae8e9fb328edca285340a5bfbf72028cc",
        "drupal/config_update": "~1.2",
        "drupal/default_content": "~1.0",
        "drupal/devel": "~1.0@alpha",
        "drupal/drupal-extension": "dev-master#203929d9d71bc0f30ff2c73a8053727b014451e1",
        "jcalderonzumba/gastonjs": "^1.1@dev",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "pear/http_request2": "~2.3",
        "pfrenssen/phpcs-pre-push": "1.1",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Drupal\\joinup\\": "tests/src",
            "DrupalProject\\": "src",
            "PhpOffice\\PhpSpreadsheet\\": "vendor/phpoffice/phpspreadsheet/src/PhpSpreadsheet"
        },
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"],
            "web/themes/joinup/vendor/{$name}": ["type:drupal-theme-library"]
        },
        "patches": {
            "drupal/core": {
                "Enlarge file_usage entity id length.": "https://www.drupal.org/files/issues/drupal-enlarge_file_usage_table-2675600.patch",
                "URI ids in entity references.": "https://www.drupal.org/files/issues/2756401-11.patch",
                "#2789315: Split off EntityPublished from node and comment into separate interface": "https://www.drupal.org/files/issues/entitypublishedinterface-2789315.patch",
                "#2346893: Duplicate AJAX wrapper around a file field": "https://www.drupal.org/files/issues/duplicate_ajax_wrapper-2346893-176.patch",
                "Don't let the whole migration crash on a download failure @see https://www.drupal.org/node/2826200": "https://www.drupal.org/files/issues/2826200-10.patch",
                "Properly check if a destination folder for a file exists": "https://www.drupal.org/files/issues/2828542-2.patch"
            },
            "drupal/r4032login": {
                "Fix white screen of death.": "https://www.drupal.org/files/issues/AccessDeniedExpression-cause-blank-screen-2687155-4.patch"
            },
            "drupal/search_api": {
                "Add entity type list cache tags.": "https://www.drupal.org/files/issues/set_list_cache_tags-2753667-8.patch",
                "Views cached results are not taking into account the access check": "./resources/patches/views_cached_results-2824640-2_0-on_2753667-8.patch"
            }
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "_comment": "The PhingDrushTask module does not currently have a D8 release and is not available on https://packages.drupal.org/8. See https://www.drupal.org/node/2817131.",
            "type": "vcs",
            "url": "git@github.com:pfrenssen/phingdrushtask.git"
        },
        {
            "type": "package",
            "package": {
                "name": "typhonius/behattask",
                "version": "1.0",
                "source": {
                    "url": "https://gist.github.com/5719e15be9687ff7c138.git",
                    "type": "git",
                    "reference": "0ea666882cba3099dfa4feeb02e1bb85b2c0bde9"
                },
                "autoload": { "classmap": [ "BehatTask.php" ] },
                "include-path": [ "." ]
            }
        },
        {
            "type": "package",
            "package": {
                "name": "pfrenssen/phpcs-pre-push",
                "version": "1.1",
                "type": "git-hook",
                "source": {
                    "url": "https://gist.github.com/1ec87c25930861654cc94251da69f14b.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "vcs",
            "url": "git@github.com:ec-europa/og_menu"
        },
        {
        "type": "package",
            "package": {
                "name": "phpoffice/phpspreadsheet",
                "version": "0.0",
                "dist": {
                    "url": "https://github.com/PHPOffice/PhpSpreadsheet/archive/develop.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "google/material-design-lite",
                "version": "dev-v1.1.3",
                "type": "drupal-theme-library",
                "source": {
                    "url": "git@github.com:google/material-design-lite.git",
                    "type": "git",
                    "reference": "v1.1.3"
                }
            }
        }
    ]
}
