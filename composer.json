    "autoload": {
        "classmap": [
            "web/modules/contrib/error_page/src/ErrorPageErrorHandler.php",
            "web/modules/contrib/sparql_entity_storage/src/Driver/Database/sparql/ConnectionInterface.php",
            "web/modules/custom/joinup_publication_date/src/Entity/EntityPublicationTimeFallbackTrait.php",
            "web/modules/custom/joinup_publication_date/src/Entity/EntityPublicationTimeInterface.php",
            "web/modules/custom/joinup_publication_date/src/Entity/EntityPublicationTimeTrait.php",
            "web/modules/custom/joinup_sparql/src/Driver/Database/joinup_sparql/Connection.php",
            "web/modules/custom/topic/src/Entity/TopicReferencingEntityInterface.php",
            "web/modules/custom/topic/src/Entity/TopicReferencingEntityTrait.php"
        ],
        "psr-4": {
            "Joinup\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "web/modules/contrib/message_digest/src/Traits/MessageDigestContextTrait.php",
            "web/modules/contrib/message_digest/src/Traits/MessageDigestTrait.php",
