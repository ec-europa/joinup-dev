# Ignore directories generated by Composer
/bin/
/drush/contrib/
/vendor/
/web/core/
/web/libraries/
/web/modules/contrib/
/web/profiles/contrib/
/web/themes/contrib/

# Ignore dist build location
/dist/

# Ignore user's local files
/local/

# Ignore sensitive information
/web/sites/sites.php
/web/sites/*/settings.php
/web/sites/*/settings.override.php

# Ignore Drupal's file directories.
/web/sites/*/files/
/private/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest/

# Ignore environment specific settings
/.env
/.env.pipeline
/behat.yml
/grumphp.yml
/phpcs.xml
/runner.yml
/web/sites/default/services.yml

# Ignore compiled CSS.
/web/themes/joinup/css

# Ignore theme libraries
/web/themes/joinup/vendor

# Ignore files generated by PhpStorm
/.idea/

# Ignore the config_readonly killswitch.
/disable-config-readonly

# Ignore temporary and auto-generated files.
/tmp/
/VERSION

# Ignore local docker compose file.
/docker-compose.override.yml

# Ignore project specific temporary generated files.
/latest.sh1
/temporary_inputfile.txt

# Used by the CI for artifacts.
/debug/
