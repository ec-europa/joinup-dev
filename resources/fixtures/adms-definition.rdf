    <rdfs:label xml:lang="en">Asset Distribution - format</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/dc/terms/MediaTypeOrExtent"/>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/dcat#Distribution"/>
    <ns1:usageNote xml:lang="en">Recommended Property - 0..1</ns1:usageNote>
  </rdf:Property>

